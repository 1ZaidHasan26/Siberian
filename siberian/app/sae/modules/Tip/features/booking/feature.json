    "icons": [
        "icons/tip1-flat.png",
        "icons/tip2-flat.png",
        "icons/tip3-flat.png"
    ],
    "files": [
        "js/controllers/tip.js",
        "js/factory/tip.js"
    ],
    "compile": true,
    "use_account": false,
    "only_once": false
}
