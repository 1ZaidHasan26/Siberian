{
    "name": "Fan-wall",
    "code": "fanwall",
    "version": "4.16.5",
    "category": "social",
    "model": "Comment_Model_Comment",
    "desktop_uri": "comment/application/",
    "routes": [
        {
            "state": "fanwall-list",
            "controller": "FanwallListController",
            "url": "fanwall/mobile_list/index/value_id/:value_id",
            "template": "list.html",
            "layouts": {
                "1": "l1",
                "2": "l2",
                "3": "l5",
                "4": "l6"
            },
            "cache": true,
            "root": true
        },
        {
            "state": "fanwall-view",
            "controller": "FanwallViewController",
            "url": "fanwall/mobile_view/index/value_id/:value_id/comment_id/:comment_id",
            "template": "l1/view.html",
            "cache": true
        },
        {
            "state": "fanwall-comment",
            "controller": "FanwallCommentController",
            "url": "fanwall/mobile_comment/index/value_id/:value_id/comment_id/:comment_id",
            "template": "l1/comment.html",
            "cache": true
        },
        {
            "state": "fanwall-gallery",
            "controller": "FanwallGalleryController",
            "url": "fanwall/mobile_comment/index/value_id/:value_id/comment_id/:comment_id",
            "template": "l1/gallery.html",
            "cache": true
        },
        {
            "state": "fanwall-map",
            "controller": "FanwallMapController",
            "url": "fanwall/mobile_map/index/value_id/:value_id",
            "template": "l1/maps.html",
            "cache": true
        },
        {
            "state": "fanwall-edit",
            "controller": "FanwallEditController",
            "url": "fanwall/mobile_edit/value_id/:value_id",
            "template": "l1/edit.html",
            "cache": false
        }
    ],
    "layouts": [
        1,
        2,
        3,
        4
    ],
    "icons": [
        "icons/fanwall1-flat.png",
        "icons/fanwall2-flat.png",
        "icons/fanwall3-flat.png"
    ],
    "files": [
        "js/controllers/job-list-controller.js",
        "js/controllers/job-view-controller.js",
        "js/controllers/company-view-controller.js",
        "js/factory/job.js",
        "scss/job.scss"
    ],
    "is_ajax": true,
    "compile": true,
    "use_account": false,
    "only_once": false
}
